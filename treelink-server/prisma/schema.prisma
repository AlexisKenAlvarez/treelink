generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider  = "postgresql"
  url       = env("POSTGRES_PRISMA_URL")
  directUrl = env("POSTGRES_URL_NON_POOLING")
}

model Users {
  email         String  @unique
  username      String?
  name          String
  bio           String?
  image         String?
  id            Int     @id @default(autoincrement())
  profile_title String

  @@index([email])
}
